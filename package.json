{
    "name": "salakala",
    "bin": {
        "salakala": "dist/cli.js"
    },
    "version": "1.3.3",
    "description": "Generate environment variables from various secret providers via URIs in JSON files, checked into your repository",
    "type": "module",
    "scripts": {
        "build": "node set_version.cjs && tsc",
        "build:clean": "rm -rf dist && node set_version.cjs && tsc",
        "test": "vitest run",
        "test:debug": "vitest run --disable-console-intercept",
        "prepare": "npm run build"
    },
    "homepage": "https://github.com/auth70/salakala",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/auth70/salakala.git"
    },
    "contributors": [
        {
            "name": "miunau",
            "email": "miunau+npm@miunau.com",
            "url": "https://miunau.com"
        }
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^24.8.1",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.911.0",
        "@azure/identity": "^4.13.0",
        "@azure/keyvault-secrets": "^4.10.0",
        "@commander-js/extra-typings": "^14.0.0",
        "@google-cloud/secret-manager": "^6.1.1",
        "@inquirer/prompts": "^7.9.0",
        "commander": "^14.0.1",
        "dotenv": "^17.2.3",
        "node-vault": "^0.10.9"
    },
    "overrides": {
        "whatwg-url": "^15.1.0"
    }
